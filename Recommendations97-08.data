INSERT INTO Recommendations97_08 (ID, CID, CNo, FY, Recommendation, LegislationRequired, LegislationStatus, Remarks) VALUES (10551,33352,21528,"2008","Continue","   ",NULL,"Due to the last minute unavailability of a quorum, the Missile Defense Advisory Committee meeting scheduled for Thursday, October 11 and Friday, October 12, 2008, from 8 a.m. to 5 p.m. was cancelled. At this date, neither the Department of Defense nor the committees Designated Federal Officer has plans to reschedule the meeting.

